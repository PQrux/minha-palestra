{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "serve": "firebase serve --only functions",
    "shell": "firebase functions:shell",
    "start": "firebase use development && firebase serve --only functions",
    "start:production": "firebase use production && firebase serve --only functions",
    "deploy": "firebase use development && firebase deploy --only functions",
    "deploy:production": "firebase use production && firebase deploy --only functions",
    "logs": "firebase use development && firebase functions:log",
    "logs:production": "firebase use production && firebase functions:log",
    "updatemodels": "cp -r ../models-minha-palestra/* ./models-minha-palestra && npm install models-minha-palestra"
  },
  "dependencies": {
    "firebase-admin": "^8.0.0",
    "firebase-functions": "^3.1.0",
    "html-pdf": "^2.2.0",
    "jsdom": "^16.2.2",
    "models-minha-palestra": "file:models-minha-palestra",
    "qrcode": "^1.4.4"
  },
  "engines": {
    "node": "10"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.1.6"
  },
  "private": true
}
